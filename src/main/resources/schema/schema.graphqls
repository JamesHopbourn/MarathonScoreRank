type Query {
    teams : [Team]
    personals(query: String, value: String) : [Personal]
}

type Personal {
    team_name: String
    personal_bib: String
    personal_name: String
    gender: String
    record_time: String
    gender_rank: Int
    overall_rank: Int
    video_rank: Int
    net_time: String
    avg_pace: String
}

type Team {
    rank: Int
    net_time: String
    team_name: String
}